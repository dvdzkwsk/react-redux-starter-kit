{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "start": "neutrino start --presets neutrino-preset-react",
    "build": "neutrino build --presets neutrino-preset-react",
    "test": "neutrino test --presets neutrino-preset-react neutrino-preset-karma",
    "lint": "eslint src",
    "deploy": "npm run lint && npm run test && npm run clean && npm run build",
    "clean": "rimraf build",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "config": {
    "presets": [
      "neutrino-preset-react",
      "neutrino-preset-karma",
      "neutrino-custom.js"
    ],
    "neutrino": {
      "output": {
        "filename": "[name].[hash].js",
        "publicPath": "/"
      },
      "devServer": {
        "watchOptions": {
          "aggregateTimeout": 300,
          "poll": 1000
        }
      },
      "module": {
        "rule": {
          "styles": {
            "test": "\\.scss$",
            "loader": {
              "style": {
                "loader": "style-loader"
              },
              "css": {
                "loader": "css-loader",
                "options": {
                  "sourceMap&-minimize": true
                }
              },
              "postcss": {
                "loader": "postcss-loader"
              },
              "sass": {
                "loader": "sass-loader",
                "options": {
                  "sourceMap": true
                }
              }
            }
          }
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "fuzzysearch": "^1.0.3",
    "js-cookie": "^2.1.3",
    "nprogress": "^0.2.0",
    "react": "^15.0.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.0.0",
    "react-fontawesome": "^1.5.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "codecov": "^1.0.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "neutrino": "^4.2.0",
    "neutrino-preset-karma": "^4.2.0",
    "neutrino-preset-react": "^4.2.0",
    "node-sass": "^4.5.0",
    "npm-check-updates": "^2.10.3",
    "postcss-loader": "^1.3.2",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2"
  }
}
